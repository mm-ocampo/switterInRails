= form_for @swit do |f|
  - if @swit.errors.any?
    #error_explanation
      %h2= "#{pluralize(@swit.errors.count, "error")} prohibited this swit from being saved:"
      %ul
        - @swit.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :username
    = f.text_field :username
  .field
    = f.label :swit
    = f.text_field :swit
  .actions
    = f.submit 'Swit'
